<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SingleNumberActivity">

    <RadioGroup
        android:id="@+id/radioGroup1_s"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="160dp"
        android:gravity="center"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <RadioButton
            android:id="@+id/icon_number3_s"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_margin="20dp"
            android:background="@drawable/radio_background"
            android:button="@null"
            android:gravity="center"
            android:text="@string/three"
            android:textColor="#fff"
            android:textSize="22dp" />

        <RadioButton
            android:id="@+id/icon_number4_s"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_margin="20dp"
            android:background="@drawable/radio_background"
            android:button="@null"
            android:gravity="center"
            android:text="@string/four"
            android:textColor="#fff"
            android:textSize="22dp" />

        <RadioButton
            android:id="@+id/icon_number5_s"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_margin="20dp"
            android:background="@drawable/radio_background"
            android:button="@null"
            android:gravity="center"
            android:text="@string/five"
            android:textColor="#fff"
            android:textSize="22dp" />

    </RadioGroup>

    <RadioGroup
        android:id="@+id/radioGroup2_s"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:orientation="horizontal"
        android:gravity="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/radioGroup1_s">

        <RadioButton
            android:id="@+id/icon_number6_s"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:background="@drawable/radio_background"
            android:button="@null"
            android:gravity="center"
            android:textSize="22dp"
            android:layout_margin="20dp"
            android:textColor="#fff"
            android:text="@string/six" />

        <RadioButton
            android:id="@+id/icon_number7_s"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:background="@drawable/radio_background"
            android:button="@null"
            android:gravity="center"
            android:textSize="22dp"
            android:layout_margin="20dp"
            android:textColor="#fff"
            android:text="@string/seven" />

        <RadioButton
            android:id="@+id/icon_number8_s"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:background="@drawable/radio_background"
            android:button="@null"
            android:gravity="center"
            android:textSize="22dp"
            android:layout_margin="20dp"
            android:textColor="#fff"
            android:text="@string/eight" />

    </RadioGroup>

    <RadioGroup
        android:id="@+id/radioGroup3_s"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:orientation="horizontal"
        android:gravity="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/radioGroup2_s">

        <RadioButton
            android:id="@+id/icon_number9_s"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:background="@drawable/radio_background"
            android:button="@null"
            android:gravity="center"
            android:textSize="22dp"
            android:layout_margin="20dp"
            android:textColor="#fff"
            android:text="@string/nine" />

        <RadioButton
            android:id="@+id/icon_number10_s"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:background="@drawable/radio_background"
            android:button="@null"
            android:gravity="center"
            android:textSize="22dp"
            android:layout_margin="20dp"
            android:textColor="#fff"
            android:text="@string/ten" />

        <RadioButton
            android:id="@+id/icon_number11_s"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_margin="20dp"
            android:background="@drawable/radio_background"
            android:button="@null"
            android:gravity="center"
            android:textSize="22dp"
            android:textColor="#fff"
            android:text="@string/eleven" />

    </RadioGroup>

    <RadioGroup
        android:id="@+id/radioGroup4_s"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="402dp"
        android:orientation="horizontal"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/radioGroup3_s"
        app:layout_constraintVertical_bias="0.0">

        <RadioButton
            android:id="@+id/icon_number12_s"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:background="@drawable/radio_background"
            android:button="@null"
            android:gravity="center"
            android:textSize="22dp"
            android:layout_margin="20dp"
            android:textColor="#fff"
            android:text="@string/twelve" />

    </RadioGroup>


    <Button
        android:id="@+id/start_s"
        android:layout_width="210dp"
        android:layout_height="46dp"
        android:layout_marginStart="101dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="101dp"
        android:layout_marginBottom="80dp"
        android:text="@string/start"
        android:textAllCaps="false"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/radioGroup4_s"
        app:layout_constraintVertical_bias="0.638" />

    <TextView
        android:id="@+id/textView_title5_s"
        android:layout_width="171dp"
        android:layout_height="72dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="88dp"
        android:layout_marginEnd="8dp"
        android:gravity="center"
        android:text="@string/instruction"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <ImageView
        android:id="@+id/imageView2_m"
        android:layout_width="101dp"
        android:layout_height="57dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="8dp"
        android:background="#00000000"
        android:scaleType="fitXY"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/button_return" />

</android.support.constraint.ConstraintLayout>
